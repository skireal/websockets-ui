{
  "name": "ws",
  "version": "1.0.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "type": "module",
  "scripts": {
    "start": "node --loader ts-node/esm src/index.ts",
    "start:dev": "nodemon -I --exec node --trace-warnings -r ts-node/register/transpile-only --loader ts-node/esm ./index.ts",
    "start:prod": "tsc && node dist/index.ts",
    "build": "tsc",
    "test": "cross-env TEST_ENV=test jest --silent"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@types/ws": "^8.5.10",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "ws": "^8.8.0"
  }
}
